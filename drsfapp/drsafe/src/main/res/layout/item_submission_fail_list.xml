<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="15dp"
    android:paddingBottom="15dp"
    android:layout_marginBottom="15dp"
    android:orientation="vertical"
    android:background="@drawable/back_rect_blue_corner">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanum_square_extra_bold"
            android:text="@string/upload_time"
            android:textColor="@color/black"
            android:textSize="14sp" />

        <!--        tvResult-->
        <!--        android:text="2021.05.07. 09:27:49 / 성공"-->
        <TextView
            android:id="@+id/tvUploadTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"

            android:fontFamily="@font/nanum_square_regular"
            android:gravity="right"
            android:textColor="@color/black"
            android:textSize="14sp"
            tools:text="2022.05.07 14:35:15" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="@string/trans_file_name"
            android:textSize="14sp"
            android:fontFamily="@font/nanum_square_extra_bold" />

        <!--        tvResult-->
        <!--        android:text="2021.05.07. 09:27:49 / 성공"-->
        <TextView
            android:id="@+id/tv_tripFileName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"

            tools:text="20210506_전북12바3_456_S_02_emath2k.TXT"
            android:textSize="14sp"
            android:gravity="right"
            android:layout_marginLeft="5dp"
            android:fontFamily="@font/nanum_square_regular" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/btn_color"
            android:text="@string/trip_file_content"
            android:textSize="14sp"
            android:fontFamily="@font/nanum_square_extra_bold" />
        <!--        tv_tripFileName-->
        <TextView
            android:id="@+id/tv_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/adapter_text_color"
            tools:text="20210506_전북12바3_456_S_02_emath2k.TXT"
            android:textSize="14sp"
            android:gravity="right"
            android:layout_marginLeft="5dp"
            android:fontFamily="@font/nanum_square_regular" />
    </LinearLayout>

</LinearLayout>